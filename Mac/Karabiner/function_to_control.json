{
  "title": "fn keys to control keys",
  "rules": [
    {
      "description": "fn+c to control+c",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "c",
              "modifiers": "control"
            }
          ]
        }
      ]
    },
    {
      "description": "fn+x to control+x",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": [
                "fn"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "x",
              "modifiers": "control"
            }
          ]
        }
      ]
    }
  ]
}
